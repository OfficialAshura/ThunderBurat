local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/RedzLibV4/main/Source.lua"))()

Library:SetTheme("Dark") -- https://raw.githubusercontent.com/REDzHUB/RedzLibV4/main/Themes.lua

Library:SetTransparency(0.1) -- 0, 1

Library:GetThemes() -- void, return = table

Library:GetIcon("Thunder")

-- ///////// 

Library.info.PlaceName

Library.info.Version

Instance:Destroy()

Instance:Visible(false)

local Window = redzlib:MakeWindow({
  Title = "ThunderBurat",
  SubTitle = "by : Ashura",
  LoadText = "ThunderBurat",
  Flags = "ThunderBurat | Example.lua"
})

--[[
  Window:Set("ThunderBurat")
]]

local Notify = Library:MakeNotify({
  Title = "ThunderHub",
  Text = "ThunderHub executed",
  Time = 3
})

--[[
  Notify:Wait() -- Wait for the notification to end
]]

local Tab = Window:MakeTab({Name = "Main", Icon = "Home"})

--[[
  Tab:Set("Main")
]]

local Toggle = MainTab:AddToggle({
  Name = "AutoRoll",
  Default = false,
  Callback = function(Value)
while true do
game:GetService("Players").LocalPlayer:WaitForChild("Rolling Egg Opening")
wait()
  end
})

--[[
  Toggle:Set(false)
  
  Toggle:Callback(function(Value)
  
  end)
]]

MainTab:AddDiscordInvite({
  DiscordTitle = "ThunderBurat",
  DiscordIcon = "rbxassetid://15298567397",
  DiscordLink = "https://discord.gg/wSzHTE6sr7"
})

Window:AddMinimizeButton({
  Button = {
    -- Button Properties
    Image = "rbxassetid://9142678957"
  },
  UICorner = {true,
    -- Corner Properties
    CornerRadius = UDim.new(0.5, 0)
  },
  UIStroke = {True, {
    -- Stroke Properties
  }}
})

local Tab = Window:MakeTab({Name = "Player", Icon = "Player"})

--[[
  Tab:Set("Player")
]]

